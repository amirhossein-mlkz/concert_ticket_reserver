    <string notr="true">/* ====== GLOBAL THEME (Dark Navy + Gray) ====== */
QWidget {
    background-color: #1b2430; /* سرمه‌ای تیره و مدرن */
    font-family: &quot;Segoe UI&quot;, &quot;Roboto&quot;, &quot;Helvetica Neue&quot;, Arial;
    font-size: 14px;
    font-weight: bold;
    color: #eaeaea; /* خاکستری روشن برای متن */
}

#info_frame, #bot_info{
	background-color: rgb(48, 65, 86);
	padding: 10px;
}

/* ====== LABEL ====== */
QLabel {
    color: #f5f5f5;
    font-weight: bold;
    background-color: transparent;
}

QLabel[role=&quot;title&quot;] {
    font-size: 18px;
    font-weight: bold;
    color: #ffffff;
}

QLabel[role=&quot;subtitle&quot;] {
    font-size: 13px;
    color: #b0b8c1;
}

/* ====== LINE EDIT ====== */
QLineEdit {
    border: 1px solid #2f3b52;
    border-radius: 8px;
    padding: 6px 10px;
    background-color: #2a3445;
    color: #f5f5f5;
    selection-background-color: #00bcd4;
    selection-color: black;
}

QLineEdit:focus {
    border: 1px solid #3ddcff;
}

/* ====== SPIN BOX ====== */
QSpinBox {
    border: 1px solid #2f3b52;
    border-radius: 8px;
    padding: 6px;
    background-color: #2a3445;
    color: #f5f5f5;
}

/* مخفی کردن دکمه‌های بالا/پایین */
QSpinBox::up-button,
QSpinBox::down-button {
    width: 0px;
    height: 0px;
    border: none;
    background: none;
}

/* فوکوس */
QSpinBox:focus {
    border: 1px solid #3ddcff;
}

/* ====== COMBO BOX ====== */
QComboBox {
    border: 1px solid #2f3b52;
    border-radius: 8px;
    padding: 6px 10px;
    background-color: #2a3445;
    color: #f5f5f5;
}

QComboBox::drop-down {
    border: none;
    width: 24px;
    background-color: #2a3445;
}

QComboBox:hover {
    border: 1px solid #3ddcff;
}

/* ====== BUTTONS ====== */
QPushButton {
    background-color: #3ddcff;
    color: #0d1b2a;
    border: none;
    border-radius: 8px;
    padding: 8px 14px;
    font-weight: bold;
}

QPushButton:hover {
    background-color: #2fb8d9;
}

QPushButton:pressed {
    background-color: #2496b3;
}

/* ====== CHECKBOX ====== */
QCheckBox {
    spacing: 8px;
    color: #eaeaea;
}

QCheckBox::indicator {
    width: 18px;
    height: 18px;
    border-radius: 4px;
    border: 1px solid #2f3b52;
    background-color: #2a3445;
}

QCheckBox::indicator:checked {
    background-color: #3ddcff;
    image: url(:/icons/icons/icons8_done_26px.png);
}
</string>
